{
    "name": "aport-website",
    "author": "SeverusVape",
    "license": "MIT",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-favicon": "^1.0.1",
        "react-icons": "^4.4.0",
        "react-scripts": "5.0.1",
        "web-vitals": "^2.1.4"
    },
    "scripts": {
        "start": "set PORT=3006 && react-scripts start",
        "watch:sass": "node-sass src/sass/main.scss src/index.css -w",
        "devserver": "npm-run-all --parallel start watch:sass",
        "build": "react-scripts build",
        "eject": "react-scripts eject"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "node-sass": "^7.0.3",
        "npm-run-all": "^4.1.5",
        "sass": "^1.54.9"
    }
}
